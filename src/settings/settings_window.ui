<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="SettingsWindow" parent="GtkBox">
    <child>
      <object class="GtkNotebook">
        <property name="vexpand">true</property>
        <property name="hexpand">true</property>
        <!---->
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkButton" id="button_install">
                <property name="label">Install a Plugin</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label">Installed</property>
                <property name="margin">5</property>
              </object>
            </child>
            <child>
              <object class="GtkScrolledWindow">
                <child>
                  <object class="GtkListBox" id="listbox_installed">
                    <property name="vexpand">true</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="label">Plugins</property>
          </object>
        </child>
        <!---->
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkLabel">
                <property name="label">Cache</property>
                <property name="margin">5</property>
              </object>
            </child>
            <child>
              <object class="GtkScrolledWindow">
                <property name="hscrollbar-policy">never</property>
                <child>
                  <object class="GtkListBox" id="listbox_cache">
                    <property name="selection-mode">none</property>
                    <property name="vexpand">true</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="label">Cache</property>
          </object>
        </child>
        <!---->
        <child>
          <object class="GtkLabel">
            <property name="label">Tmdb Settings</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="label">Tmdb</property>
          </object>
        </child>
        <!---->
        <child>
          <object class="GtkLabel">
            <property name="label">Player Settings</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="label">Player</property>
          </object>
        </child>
        <!---->
      </object>
    </child>
  </template>
</interface>
